<body>

	<div class="container">
		<center>
			<div class="row mt-5">
				<div class="col-sm-4">
					<ul>
						<li><a href="javascript:history.back()" class="black"><i class="fas fa-arrow-alt-circle-left fa-lg"></i><p>Back</p></a></li>
					</ul>
				</div>
				<div class="col-sm-4"><h1>Profile</h1></div>
				<div class="col-sm-4 center-block">
				{{#if viewOLD}}
					<ul>
						<li><a id="SettingsButtonOLD" href="alertsettings" class="black"><i class="fas fa-cogs fa-lg"></i><p>Settings</p></a></li>
						<li><i id="HelpButtonOLD" class="fas fa-info-circle fa-lg" onclick="helpProfile()" onmouseover="" style="cursor: pointer;"></i><p onclick="helpProfile()" onmouseover="" style="cursor: pointer;">Help</p></li>
						<li><a id="LogoutButtonOLD" href="/" class="black"><i class="fas fa-sign-out-alt fa-lg"></i><p>Logout</p></a></li>
					</ul>
				{{/if}}
				</div>
			</div>

				<img src="/images/cogs120_profile1.png"  width="200">

				<img src="/images/cogs120_prof2.png"  width="200"><br><br>

				<img src="/images/cogs120_prof3.png"  width="200">
				<div onload="displayProfile()" id="display">
				</div>
{{#if viewNEW}}
<div class="container-fluid">
				<div class="row icons">
					<div id="SettingsButtonNEW" class="col-3"><a href="alertsettings" class="white"><i class="fas fa-cogs fa-lg"></i><p>Settings</p></a></div>
					<div id="HelpButtonNEW" class="col-3"><i class="fas fa-info-circle fa-lg" onclick="helpSettings()" onmouseover="" style="cursor: pointer;"></i><p onclick="helpSettings()" onmouseover="" style="cursor: pointer;">Help</p></div>
					<div id="ProfileButtonNEW" class="col-3"><a href="profile" class="white" disabled><i class="fas fa-user fa-lg"></i><p>Profile</p></a></div>
					<div id="LogoutButtonNEW" class="col-3"><a href="/" class="white"><i class="fas fa-sign-out-alt fa-lg"></i><p>Logout</p></a></div>
				</div>
			</div>


		</center>
	</div>
{{/if}}

