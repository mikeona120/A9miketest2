	<title>SoundTrack</title>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link href="/css/bootstrap.min.css" rel="stylesheet">
	<link href="/css/bootstrap-theme.min.css" rel="stylesheet">
	<link rel="stylesheet" type="text/css" href="/css/styles.css">
	<link href="https://fonts.googleapis.com/css?family=Dosis" rel="stylesheet">
	<link href="https://use.fontawesome.com/releases/v5.0.6/css/all.css" rel="stylesheet">
	

</head>

<body>
	<body onload="playSavedSoundscape()">
		<div class="container">
			<center>
				<div class="row mt-3">
					<div class="col-sm-4"></div>
					<div class="col-sm-4">
						<h1>SoundTrack</h1>
	
					</div>
					<div class="col-sm-4">
						{{#if viewOLD}}
						<ul>
							<li><a id="SettingsButtonOLD" href="alertsettings" class="black"><i class="fas fa-cogs fa-lg"></i><p>Settings</p></a></li>
							<li><i id="HelpButtonOLD" class="fas fa-info-circle fa-lg" onclick="helpListen()" onmouseover="" style="cursor: pointer;"></i><p onclick="helpListen()" onmouseover="" style="cursor: pointer;">Help</p></li>
							<li><a id="ProfileButtonOLD" href="profile" class="black"><i class="fas fa-user fa-lg"></i><p>Profile</p></a></li>
							<li><a id="LogoutButtonOLD" href="/" class="black"><i class="fas fa-sign-out-alt fa-lg"></i><p>Logout</p></a></li>
						</ul>
						{{/if}}
					</div>
				</div>


				<div id="productivityTracker" class="row mt-4">
					<h4 class="col-12 mb-3">Productivity Tracker</h4>
					<div class="productivity col-12"><b>Productive for: </b><span id="productiveTime">00:00</span></div>
					<div class="offtask col-12"><b>Total Break Time: </b><span id="timer"></span></div>
				</div>

				

				<br>
				<h4 data-toggle="tooltip" title="Get your head in the game! Choose a relaxing sound for your work session.">Soundscape</h4>

				<div class="soundscroll squares" id="scroll1">
					{{#each soundscapes}} 
					<button class="btn btn-outline-secondary scape" onclick="soundEffect({{soundEffectnum}})" id="{{id}}"><i class='{{icon}} fa-lg'></i><br>{{name}}</button>
					{{/each}}
				</div>

		<!-- 		<div>
				<marquee scrollamount="10"direction="left"behavior="scroll">
				<img src="gif/lol.gif"/>Relax Relax Relax~~~~~
				</marquee>
				</div>

				<div>
				<marquee scrollamount="10"direction="right"behavior="scroll">
				Relax Relax Relax~~~~~<img src="gif/potato.gif"/>
				</marquee>
				</div> -->

				<br>
				<a href="home" class="btn btn-outline-primary float" role="button" aria-pressed="true" onclick="resetPTimer()">Finish Work Session</a>
			</div>
{{#if viewNEW}}
			<div class="container-fluid">
				<div class="row icons">
					<div id="SettingsButtonNEW" class="col-3"><a href="alertsettings" class="white"><i class="fas fa-cogs fa-lg"></i><p>Settings</p></a></div>
					<div id="HelpButtonNEW" class="col-3"><i class="fas fa-info-circle fa-lg" onclick="helpListen()" onmouseover="" style="cursor: pointer;"></i><p onclick="helpListen()" onmouseover="" style="cursor: pointer;">Help</p></div>
					<div id="ProfileButtonNEW" class="col-3"><a href="profile" class="white" disabled><i class="fas fa-user fa-lg"></i><p>Profile</p></a></div>
					<div id="LogoutButtonNEW" class="col-3"><a href="/" class="white"><i class="fas fa-sign-out-alt fa-lg"></i><p>Logout</p></a></div>
				</div>
			</div>
{{/if}}

		</center>
	</div>




	<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
	<script src="/js/bootstrap.min.js"></script>
	<script src="/js/screen2.js"></script>
	<script src="/js/sounds.js"></script>
</body>
</html>